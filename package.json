    "enzyme": "~3.10.0",
    "enzyme-adapter-react-16": "~1.14.0",
    "eslint": "~6.0.1",
    "eslint-config-standard": "~12.0.0",
    "eslint-plugin-emotion": "~10.0.14",
    "eslint-plugin-import": "~2.18.0",
